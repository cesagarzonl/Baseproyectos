							p Hola #{user.username} estos son los datos que tienes registrados 
							p Telefono: #{user.telefono} 
							p Email: #{user.email}
							p  nit : #{user.nit}
					.divicion(style="text-align: center;" )
								.risponsive
									div
										h2 Va dirigido a:
									div(id="especificos" name="especificos")
											if  user.radio32 == "on"
												div
													label(for="radio32")
														img(src="/public/imagenes/img/DOG.png")
													| Perros
											if  user.radio28 == "on"
												div
													label(for="radio28")
														img(src="/public/imagenes/img/CAT.png")
													| Gatos
											if  user.radio29 == "on"
												div
													label(for="radio29")
														img(src="/public/imagenes/img/HAMSTER.png")
													| Hámsters
											if  user.radio30 == "on"
												div
													label(for="radio30")
														img(src="/public/imagenes/img/BIRD.png")
													| Pajaros
											if  user.radio31 == "on"	
												div
													label(for="radio31")
														img(src="/public/imagenes/img/PES.png")
													| Peces
									div
										h2 Servicios especificos
									div(id="especificos" name="especificos")
										if  user.radio1 == "on" 
												div
													img(src="/public/imagenes/img/icon/Iconos-13.png")
													p Corte
										if  user.radio1 == "on" 
												div
													img(src="/public/imagenes/img/icon/Iconos-12.png")
													p Uñas
										if  user.radio8 == "on" 
												div
													img(src="/public/imagenes/img/icon/Iconos-11.png")
													p Telefono
										if  user.radio2 == "on" 

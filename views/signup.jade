doctype html
html(lang="es")
	head
		meta(charset="utf-8")
		meta(name="description" content="")
		meta(name='viewport' content='width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0')
		title WORKEY 
		link(rel="stylesheet" type="text/css" href="/public/css/bootstrap.css")
		link(rel="stylesheet" type="text/css" href="/public/css/elbusca.css")
		link(rel="shortcut icon" href="public/imagenes/img/ntede.comr.png")
	body.landing( style="overflow-x:hidden; overflow-y:scroll;")
		header
					div(class="colorarriba" align='center')
						.picture(style="margin-bottom:4px;")
								a(href='/' )
									img(src='/public/imagenes/img/logos-01.png' class="logop" id="logop" name="logop")
								ul.nav.navbar-nav.navbar-right(style="padding-left: 40%")
													li.dropdown
														input.button(value='Contacto' data-type="zoomout" readonly="readonly")
													li.dropdown
														input.buttons(value='Ingresar', data-type='zoomin' readonly="readonly")
					section#banner
						h2 LA OFICINA QUE QUIERES...
						h2 LA OFICINA QUE TIENES
						p Rápido, Fácil y flexible 
						ul.actions
							button(type="button" id="busss" class="busss active" onclick="sibuss()") Ubicación
							button(type="button" id="enlis" class="enlis" onclick="nobus()")  Publicar
						section
									div(id="nobus" class="nobus active2")
										li.nobus
											form(style='text-align: center;', action='/hosts', method='POST', label='')
														input(placeholder="Ubicación" type="search" id="mysearch1" name="mysearch" autocomplete="off" list="datos2")
														datalist(id="datos2" class="datos2")
														span.glyphicon(style='')
															input.imputt(value='PUBLICAR', type='submit')
									div(id="sibus" class="sibus")
										li.sibus
											form(style='text-align: left;', action='/extencion', method='POST', label='')
														input(placeholder="Ubicación" type="search" id="mysearch1" name="mysearch" autocomplete="off" list="datos2")
														datalist(id="datos2" class="datos2")
														span.glyphicon(style='')
															input.imputt(value='ENCONTRAR', type='submit')
				div(class="organiza")
					.overlay-containers(style="display:block;")
						.window-containers1.zoomin1.window-container-visibles1
							.sesions
								p#creacu(style='color:#fff;')
									| Ingresar
									span.close X
								form#for.for(action='session', method='POST')
									.form-group(style='padding-top: 30px; ')
										label(for='email') Correo
										input#email(name='email', autocomplete='off required', type='text')
									div
										label(for='password') Contraseña
										input#password(name='password', type='password', required='')
									div
										input#send(value='Ingresar', clas='send login', autocomplete='off', type='button')
									div
										p(style='padding-left: 25%')
											| Nuevo en workey? 
											input.rrs(value='Crea una cuenta', data-type='rrr', autocomplete='off')
					.overlay-container
					.overlay-container2
			footer
						div
							h2 Contacta a nuestro equipo para más información llámanos o escríbenos.
							h2 Todos los precios están sujetos a cambios sin previo aviso
						div
							p Tel: +(57) 3134301091
							p Email:workeybogota@gmail.com
						div(style="padding-left: 5%;")
							div(id="face" class="face" name="face")
								a(href="https://www.facebook.com/WorkeyOffice/?fref=ts")
									img(src="/public/imagenes/img/face.png" width="50px")
							div(id="face" class="face" name="face")
								a(href="https://www.linkedin.com/company/11280196/")
									img(src="/public/imagenes/img/inkedin-icon.png" width="50px")
	script(type='text/javascript', src='/public/js/jquery.min.v1.js')
